#!/usr/bin/env php
<?php
require_once __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Console\App\Commands\ExportCSVCommand;
use Console\App\Model\SalaryDetails;

$app = new Application();
$salesManager = new SalaryDetails();

$app->add(new ExportCSVCommand($salesManager));
$app->run();